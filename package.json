{
  "name": "@ssense/cucumber-testrail-sync",
  "version": "2.0.13",
  "description": "Synchronize test cases from TestRail",
  "bin": "./bin/testrail-sync",
  "main": "./lib/index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint lib/*.js",
    "test": "npm run test:unit && npm run test:bdd",
    "test:unit": "NODE_ENV=test ./node_modules/.bin/mocha",
    "test:bdd": "NODE_ENV=test ./node_modules/.bin/cucumberjs -r features --format summary --fail-fast",
    "cover": "npm run cover:unit && npm run cover:bdd",
    "cover:unit": "./node_modules/.bin/nyc --report-dir coverage/unit npm run test:unit",
    "cover:bdd": "./node_modules/.bin/nyc --report-dir coverage/bdd npm run test:bdd"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Groupe-Atallah/node-cucumber-testrail-sync.git"
  },
  "author": "Mickael Burguet <mickael.burguet@ssense.com>",
  "license": "MIT",
  "engines": {
    "node": ">= 4.0.0"
  },
  "dependencies": {
    "async": "^2.0.0-rc.6",
    "bluebird": "^3.4.6",
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "diff": "^3.0.0",
    "escape-string-regexp": "^1.0.5",
    "handlebars": "^4.0.5",
    "inquirer": "^1.0.3",
    "joi": "^8.4.2",
    "lodash": "^4.13.1",
    "mkdirp": "^0.5.1",
    "string": "^3.3.1",
    "testrail-api": "^1.0.2",
    "uniquefilename": "^1.1.0",
    "walkdir": "0.0.11"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "cucumber": "^1.2.2",
    "eslint": "^2.13.1",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.9.2",
    "glob": "^7.0.6",
    "mocha": "^2.5.3",
    "mock-fs": "^3.9.0",
    "nock": "^8.0.0",
    "nyc": "^8.1.0",
    "rewire": "^2.5.2"
  },
  "nyc": {
    "include": [
      "lib/ResultSynchronizer.js",
      "lib/ScenarioSynchronizer.js"
    ],
    "reporter": [
      "json",
      "html"
    ],
    "all": true
  }
}
