{
  "name": "@ssense/cucumber-testrail-sync",
  "version": "1.0.6",
  "description": "Synchronize test cases from TestRail",
  "bin": "./bin/testrail-sync",
  "main": "./lib/index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint lib/*.js",
    "test": "npm run test:unit && npm run test:bdd",
    "test:unit": "./node_modules/.bin/mocha",
    "test:bdd": "NODE_ENV=test ./node_modules/.bin/cucumberjs -r features --format summary --fail-fast",
    "cover": "npm run cover:unit && npm run cover:bdd",
    "cover:unit": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --report html --bail --dir coverage/unit",
    "cover:bdd": "./node_modules/.bin/istanbul cover ./node_modules/.bin/cucumberjs --report html --bail --dir coverage/bdd -- -r features --format summary"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Groupe-Atallah/node-cucumber-testrail-sync.git"
  },
  "author": "Mickael Burguet <mickael.burguet@ssense.com>",
  "license": "MIT",
  "engines": {
    "node": ">= 0.12.0"
  },
  "dependencies": {
    "async": "^2.0.0-rc.6",
    "chalk": "^1.1.3",
    "cucumber": "^1.2.2",
    "dateformat": "^1.0.12",
    "escape-string-regexp": "^1.0.5",
    "handlebars": "^4.0.5",
    "inquirer": "^1.0.3",
    "joi": "^8.4.2",
    "lodash": "^4.13.1",
    "mkdirp": "^0.5.1",
    "string": "^3.3.1",
    "testrail-api": "^1.0.1",
    "walkdir": "0.0.11"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "cucumber": "^1.0.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.9.2",
    "istanbul": "^0.4.4",
    "mock-fs": "^3.9.0",
    "nock": "^8.0.0",
    "rewire": "^2.5.2",
    "mocha": "^2.5.3"
  }
}
